/* pages/level/index.wxss */
.level-wrap {
    width: 100vw;
    height: 100vh;
}
.level-title {
    margin-top: 3.5vw;
    margin-bottom: 6vw;
    position: relative;
    font-size: 36rpx;
    height: 40rpx;
}
.level-title-text {
    position: absolute;
    width: 22vw;
    height: 100%;
    text-align: center;
    left: 40vw;
    z-index: 2;
    background: #293039;
}
.level-title-line {
    position: absolute;
    width: 30vw;
    height: 2rpx;
    background: #fff;
    left: 35vw;
    top: 24rpx;
}
.level-card-wrap {
    width: 100vw;
    overflow: hidden;
}
.level-card {
    width: 15.8vw;
    height: 15.8vw;
    border-radius: 16rpx;
    font-size: 36rpx;
    float: left;
    margin-left: 3.5vw;
    margin-bottom: 3.5vw;
    background-color: rgb(30, 39, 44);
    box-sizing: border-box;
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    overflow: hidden;
}
.level-lock {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: #2f3f47;
    display: flex;
    align-items: center;
    justify-content: center;
}
.level-lock-icon {
    width: 38%;
    height: 38%;
}
.level-card-ok {
    position: absolute;
    right: 0.2px;
    bottom: 0.2px;
    width: 46rpx;
    height: 46rpx;
    background: url(data:image/png;base64,)
        no-repeat;
    background-size: contain;
}
.level-card:active {
    background-color: #1890ff;
    color: #fff;
    transition: all 0.2s linear;
}
.level-tips {
    position: absolute;
    top: 2vw;
    left: 3.5vw;
    color: #898989;
    font-size: 20rpx;
}
.level-loading {
    position: fixed;
    height: 100vh;
    width: 100vw;
    z-index: 3;
    background-color: #293039;
    top: 0;
    opacity: 1;
}
.fadeOut {
    animation: fadeOut 1s linear forwards;
}
@keyframes fadeOut {
    0% {
        opacity: 1;
    }
    30% {
        opacity: 1;
    }
    100% {
        opacity: 0;
    }
}
